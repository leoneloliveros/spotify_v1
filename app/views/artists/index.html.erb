<% @artists.each do |artist| %>
  <div class="col-sm-3">
    <div class="card">
      <img class="card-img-top" src= "<%= artist.image_url%>" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title text-center"><%= artist.name %></h5>
        <%= link_to "Ver info", show_artists_path(artist), class: "btn btn-primary btn-block" %>
      </div>
    </div>
  </div>
<% end %>
